<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiStrides.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiStrides.Views"
    Title="MauiStrides">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage" />

    <ShellContent
        Title="Activities"
        ContentTemplate="{DataTemplate local:ActivitiesPage}"
        Route="ActivitiesPage" />

    <ShellContent
    Title="Summary Reports"
    ContentTemplate="{DataTemplate local:SummaryPage}"
    Route="SummaryPage" />

</Shell>
